    <div class="myindex__body__container">
        <div class="white__section">
            <div class="white__section__content">
                <h1> What you can do </h1>
                <div class="section__index__content wow flipInX" data-wow-offset="400">
                    <h2 class="title__index__contentsection">
                        Have fun in English with Games
                    </h2>
                    <p class="index__content__intro">
                        Learning a language on Duolingo is completely free, but you can remove ads and support free education with Plus. First 7 days on us!
                    </p>
                    <div class="selector_cover">
                        <a id="PuzzleActive" class="PuzzleGamelink">
                            <div class="PuzzlegameIcon">

                            </div>
                            <p>Puzzle Games</p>
                        </a>
                        <a id="QuizActive" class="Quizlink">
                            <div class="QuizIcon">
                            </div>
                            <p>
                                Quiz Games
                            </p>
                        </a>
                        <a id="IncomingActive" class="Quizlink">
                            <div class="Anothergame">
                            </div>
                            <p>
                                Another Game incoming
                            </p>
                        </a>
                    </div>
                </div>
                <div class="section__index__image wow flipInY " data-wow-offset="400">
                    <div class="PuzzleGamePick shawdowImage ">

                    </div>
                    <div class="QuizPick shawdowImage inviImage">

                    </div>
                    <div class="IncomingGamespick shawdowImage inviImage">

                    </div>
                </div>
            </div>
        </div>
        <div class="grey__section">
            <div class="grey__section__content">
                <div class="section__index__image wow rotateInDownLeft " data-wow-offset="400">
                    <div class="ranking__container wow bounce" data-wow-iteration="100" data-wow-duration="2s" data-wow-offset="400">

                    </div>
                </div>
                <div class="section__index__content wow rotateInDownRight" data-wow-offset="400">
                    <h2 class="title__index__contentsection">
                        Track Your Performance
                    </h2>
                    <p class="index__content__intro">
                        The world's most popular language learning platform is now available for the classroom. Thousands of teachers are already using it to enhance their lessons.
                    </p>
                    <a class="joinus_rank">
                        Join Us to get a spot in Leaderboard
                    </a>
                </div>
            </div>
        </div>
        <div class="white__section">
            <div class="white__section__content">
                <div class="section__index__content wow bounceInLeft" data-wow-offset="400">
                    <h2 class="title__index__contentsection">
                        You can create you own Test with our unique technique
                    </h2>
                    <p class="index__content__intro">
                        You're a Teacher?, let's create tests for your student<br />
                        You're a Student?, let's unlock your potential by creating your own difficult test and Take It!
                    </p>
                    <a class="joinus_rank">
                        Let's try to create a Test now
                    </a>
                </div>
                <div class="section__index__image wow bounceInRight" data-wow-offset="400">
                    <div class="TeacherCreateTest">

                    </div>
                </div>
            </div>
        </div>
    </div>
    <section class="videos">
        @foreach (var type in Model.TestTypes)
        {
            <h2 class="heading-2 videos__heading">@type</h2>
            foreach (var t in Model.Tests[Model.TestTypes.IndexOf(type)])
            {
                <div class="video__puzzleindex">
                    <h4 class="heading-4 heading-4--yellow">videopuzzle</h4>
                    <div class="video__img-box">
                        <img src="@t.Image" alt="" class="video__img">
                        <div class="video__layer"></div>
                        <a href="~/Student/TestRouting?TestId=@t.id" class="btn video__btn">play</a>
                    </div>
                    <div class="video__title">@t.Desc</div>
                    <div class="video__info">
                        <div class="video__difficulty video__difficulty--1">
                            <span class="video__difficutly-point"></span>
                            <span class="video__difficutly-point"></span>
                            <span class="video__difficutly-point"></span>
                        </div>
                        <img src="~/Content/Images/unitedkingdom.png" alt="" class="video__accent">
                        <p class="video__phrases">25 phrase</p>
                    </div>
                </div>
            }
        }
    </section>
    <footer class="footer" id="contact">
        <a href="#" class="nav__name heading-1 footer__logo">ABC English</a>
        <nav class="footer__nav">
            <ul class="footer__list">
                <li class="footer__item">
                    <a href="#home" class="footer__link">home</a>
                </li>
                <li class="footer__item">
                    <a href="#skills" class="footer__link">skills</a>
                </li>
                <li class="footer__item">
                    <a href="#projects" class="footer__link">projects</a>
                </li>
                <li class="footer__item">
                    <a href="#contact" class="footer__link">contact</a>
                </li>
            </ul>
        </nav>
        <div class="footer__details">
            <p class="footer__text">Living, learning, & leveling up one day at a time.</p>
        </div>
    </footer>
    @*</div>*@
</body>
</html>
<script>
    $(document).ready(function () {
        var scrollTop = 0;
        $(window).scroll(function () {
            scrollTop = $(window).scrollTop();
            $('.counter').html(scrollTop);

            if (scrollTop >= 100) {
                $('#global-nav').addClass('scrolled-nav');
            } else if (scrollTop < 100) {
                $('#global-nav').removeClass('scrolled-nav');
            }

        });
    });
</script>

    <div class="User__form__changeEP">
         <h2 class="EP__form__title">
             Settings
         </h2>
         <div class="User__EP__form">
             <h3 class="EP__title">
                 Personal Information
             </h3>
             <div class="EP__form__row">
                 <label class="EP__form__label">Photo</label>
                 <div class="EP__UserForm__img">
                     <img src="~/Content/Images/avatar.jpg" />
                 </div>
                 <a class="EP__upload__photo">Upload</a>
             </div>
             <div class="EP__form__row">
                 <label class="EP__form__label">Name</label>
                 <div class="EP__form__input">
                     <input />
                 </div>
             </div>
             <div class="EP__form__row">
                 <label class="EP__form__label">
                     Email
                 </label>
                 <div class="EP__form__input">
                     <input />
                 </div>
             </div>
             <div class="EP__form__row">
                 <label class="EP__form__label">
                 </label>
                 <div class="EP__form__input">
                     <button class="EP__submit" type="submit" formmethod="post">Save Changes</button>
                 </div>
             </div>
             <h3 class="EP__title">
                Change your Password
             </h3>
             <div class="myhr"> </div>
             <div class="EP__changePassForm">
                 <div class="EP__form__row">
                     <label class="EP__form__label">
                         Current Password
                     </label>
                     <div class="EP__form__input">
                         <input />
                     </div>
                 </div>
                 <div class="EP__form__row">
                     <label class="EP__form__label">
                         New Password
                     </label>
                     <div class="EP__form__input">
                         <input />
                     </div>
                 </div>
                 <div class="EP__form__row">
                     <label class="EP__form__label">
                         Confirm new Password
                     </label>
                     <div class="EP__form__input">
                         <input />
                     </div>
                 </div>
                 <div class="EP__form__row">
                     <label class="EP__form__label">
                     </label>
                     <div class="EP__form__input">
                         <a class="forgerPass">
                             Forget your password?
                         </a>
                     </div>
                     </div>
                     <div class="EP__form__row">
                         <label class="EP__form__label">
                         </label>
                         <div class="EP__form__input">
                             <button class="EP__submit" type="submit" formmethod="post">Save Changes</button>
                         </div>
                     </div>
                 </div>
         </div>
    </div>
</div>
